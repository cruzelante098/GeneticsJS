{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "rules": {
        "interface-name" : [true, "never-prefix"],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "no-console": false,
        "object-literal-sort-keys": false,
        "object-literal-shorthand": false,
        "ordered-imports": false
    }
}